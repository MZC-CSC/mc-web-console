@startuml

title "Get Menu List"

actor User as user

participant "app.go" as app
participant "adminconfig.go" as route
database Database as DB

group  "API Call"
user    -> app : GET: /api/adminconfig/menu
user    -> app : query: menuId, menuName, menuAlias, categoryId
end

app     -> route: MenuList()
route <-> DB: Fetch List
user <- route:  return menuList


@enduml
