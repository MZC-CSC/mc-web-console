@startuml

title "Get Category List"

actor User as user

participant "app.go" as app
participant "adminconfig.go" as route
database Database as DB

group  "API Call"
user    -> app : GET: /api/adminconfig/category
user    -> app : query: categoryName
end

app     -> route: CategoryList()
route <-> DB: Fetch List
user <- route:  return categoryList


@enduml
