@startuml

title "Register Menu"

actor User as user

participant "app.go" as app
participant "adminconfig.go" as route
database Database as DB

group  "API Call"
user    -> app : POST: /api/adminconfig/menu
user    -> app : body: models.Menu
end
app     -> route: RegMenu()
route <-> DB: Create
user <- route:  return successMessage


@enduml
